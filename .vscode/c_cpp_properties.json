{
    "configurations": [
        {
            "name": "STM32",
            "includePath": [
                "${workspaceFolder}/vendor",
                "${workspaceFolder}/src"
            ],
            "defines": [
                "STM32F411xE",
                "__GNUC__"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mfloat-abi=hard",
                "-mfpu=fpv4-sp-d16"
            ]
        }
    ],
    "version": 4
}
